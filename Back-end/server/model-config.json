  "Issue": {
    "dataSource": "postgres",
    "public": true
  },
  "Type": {
    "dataSource": "postgres",
    "public": true
  },
  "State": {
    "dataSource": "postgres",
    "public": true
  },
  "Notification": {
    "dataSource": "postgres",
    "public": true
  },
  "IssueMember": {
    "dataSource": "postgres",
    "public": true
  },
  "Priority": {
    "dataSource": "postgres",
    "public": true
  },
  "Importance": {
    "dataSource": "postgres",
    "public": true
  },
  "Commentary": {
    "dataSource": "postgres",
    "public": true
  },
  "Location": {
    "dataSource": "postgres",
    "public": true
  },
  "ImageService": {
    "dataSource": "images",
    "public": false
  },
  "ImageFile": {
    "dataSource": "postgres",
    "public": true
  },
  "House": {
    "dataSource": "postgres",
    "public": true
  },
  "Book": {
    "dataSource": "postgres",
    "public": true
  },
  "Auhtor": {
    "dataSource": "postgres",
    "public": true
  }
